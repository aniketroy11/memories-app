./../actions/posts';\r\nimport Posts from '../Posts/Posts';\r\nimport Form from '../Form/Form';\r\nimport Paginate from '../Pagination/Pagination';\r\n\r\nimport useStyles from './styles';\r\n\r\n\r\n\r\nfunction useQuery() {\r\n  return new URLSearchParams(useLocation().search);\r\n}\r\n\r\nconst Home = () => {\r\n\r\n  const [currentId, setCurrentId] = useState(null);\r\n\r\n  const dispatch = useDispatch();\r\n\r\n  const query = useQuery();\r\n  const navigate = useNavigate();\r\n  const page = query.get('page') || 1;\r\n  const searchQuery = query.get('searchQuery');\r\n\r\n  const classes = useStyles();\r\n\r\n  const [search,setSearch] = useState('');\r\n  const [tags,setTags] = useState([]);\r\n\r\n\r\n  useEffect(() => {\r\n    dispatch(getPosts());\r\n  }, [currentId, dispatch]);\r\n  \r\n  const searchPost =()=>{\r\n    if(search.trim()){\r\n      //dispatch -> fetch search post\r\n      dispatch(getPostsBySearch({search,tags:tags.join(',')}));\r\n      navigate(`/posts/search?searchQuery=${search || 'none'}&tags=${tags.join(',')}`)\r\n    }\r\n    else{\r\n      navigate('/');\r\n    }\r\n  }\r\n\r\n  const handleKeyPress = (e)=>{\r\n    if(e.keyCode === 13){\r\n      searchPost();\r\n    }\r\n  }\r\n\r\n  const handleAdd =(tag)=>setTags([...tags,tag]);\r\n  const handleDelete =(tagToDelete)=>setTags(tags.filter((tag)=>tag!==tagToDelete));\r\n\r\n\r\n  return (\r\n    <Grow in>\r\n      <Container maxWidth=\"xl\">\r\n        <Grid className={classes.gridContainer} container justifyContent='space-between' alignItems='stretch' spacing={3}>\r\n          <Grid item xs={12} sm={6} md={9}>\r\n            <Posts setCurrentId={setCurrentId} />\r\n          </Grid>\r\n          <Grid item xs={12} sm={6} md={3}>\r\n            <AppBar className={classes.appBarSearch} position='static' color=\"inherit\">\r\n              <TextField\r\n                name='search'\r\n                variant=\"outlined\"\r\n                label=\"Search Memories\"\r\n                fullWidth\r\n                onKeyPress={handleKeyPress}\r\n                value={search}\r\n                onChange={(e)=>setSearch(e.target.value)}\r\n                />\r\n                <ChipInput\r\n                  style={{margin:'10px 0'}}\r\n                  value={tags}\r\n                  onAdd={handleAdd}\r\n                  onDelete={handleDelete}\r\n                  label=\"Search Tags\"\r\n                  variant=\"outlined\"\r\n                 />\r\n                 <Button onClick={searchPost} className={classes.searchButton} color=\"primary\" variant=\"contained\">Search</Button>\r\n            </AppBar>\r\n            <Form currentId={currentId} />\r\n            <Paper elevation={6}>\r\n              <Paginate />\r\n            </Paper>\r\n          </Grid>\r\n        </Grid>\r\n      </Container>\r\n    </Grow>\r\n  )\r\n}\r\n\r\nexport default Home\r\n",[],"C:\\Users\\devil\\OneDrive\\Desktop\\Memories app\\client\\src\\components\\Auth\\Auth.js",[],[],"C:\\Users\\devil\\OneDrive\\Desktop\\Memories app\\client\\src\\components\\Auth\\styles.js",[],[],"C:\\Users\\devil\\OneDrive\\Desktop\\Memories app\\client\\src\\components\\Auth\\Input.js",[],[],"C:\\Users\\devil\\OneDrive\\Desktop\\Memories app\\client\\src\\components\\Auth\\Icon.js",[],[],"C:\\Users\\devil\\OneDrive\\Desktop\\Memories app\\client\\src\\reducers\\auth.js",[],[],"C:\\Users\\devil\\OneDrive\\Desktop\\Memories app\\client\\src\\components\\Home\\styles.js",[],[],"C:\\Users\\devil\\OneDrive\\Desktop\\Memories app\\client\\src\\actions\\auth.js",[],[],"C:\\Users\\devil\\OneDrive\\Desktop\\Memories app\\client\\src\\components\\pagination\\Pagination.js",[],[],"C:\\Users\\devil\\OneDrive\\Desktop\\Memories app\\client\\src\\components\\pagination\\style.js",[],[],"C:\\Users\\devil\\OneDrive\\Desktop\\Memories app\\client\\src\\components\\PostDetails\\PostDetails.jsx",[],[],"C:\\Users\\devil\\OneDrive\\Desktop\\Memories app\\client\\src\\components\\Pagination\\Pagina